            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="script">import org.apache.jmeter.reporters.ResultCollector
import org.apache.jmeter.save.SaveService
import org.apache.jmeter.util.JMeterUtils
import org.apache.jmeter.services.JMeterContextService
import java.nio.file.Paths

// Define file paths
String resultsTreePath = props.get(&quot;resultsTreeFilePath&quot;) ?: &quot;./rawResults.jtl&quot; // Default if not provided
String aggregateReportFilePath = props.get(&quot;aggregateReportFilePath&quot;) ?: &quot;./aggregateReport.csv&quot;  // Default to current dir if not provided


// Initialize JMeter utilities (ensure paths are set if running standalone)
JMeterUtils.setProperty(&quot;jmeter.save.saveservice.output_format&quot;, &quot;csv&quot;) // Ensure raw data is saved in CSV format

// Create and configure the ResultCollector for the aggregate report
ResultCollector collector = new ResultCollector()
collector.setFilename(aggregateReportFilePath)

// Set the result collector as a listener (this will handle the results in JMeter)
JMeterContextService.getContext().getVariables().put(&quot;ResultCollector&quot;, collector)

// Log file paths
println(&quot;Raw data saved to: &quot; + resultsTreePath)
println(&quot;Aggregate report saved to: &quot; + aggregateReportFilePath)

// Add the ResultCollector to the JMeter test plan for result collection
// Typically, ResultCollector would be added to the Test Plan in JMeter&apos;s GUI but if doing it programmatically, this is how to add it
SaveService.saveTestElement(collector, Paths.get(aggregateReportFilePath).toFile())
</stringProp>
