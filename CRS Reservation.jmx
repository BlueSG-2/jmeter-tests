        <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="Load Test Run Setup">
          <stringProp name="scriptLanguage">groovy</stringProp>
          <stringProp name="parameters"></stringProp>
          <stringProp name="filename"></stringProp>
          <stringProp name="cacheKey">true</stringProp>
          <stringProp name="script">import java.text.SimpleDateFormat
import java.util.Calendar

props.put(&quot;threadCount&quot;, &quot;100&quot;)

// Fetch the &apos;threadCount&apos; and &apos;environment&apos; properties passed via command line (-J options)
String threadCountStr = vars.get(&quot;threadCount&quot;) ?: props.get(&quot;threadCount&quot;) ?:  &quot;&quot; // Try bash param, then UDV, else empty
String environment = vars.get(&quot;environment&quot;) ?: props.get(&quot;environment&quot;) ?:  &quot;&quot; // Try bash param, then UDV, else empty

// Print debug information
println(&quot;threadCountStr: &quot; + props.get(&quot;threadCount&quot;) + &quot; &quot;+ vars.get(&quot;threadCount&quot;))

// Check if threadCount is provided and is a valid integer
int threadCount = -1 // Default value for threadCount if not valid

if (!threadCountStr.isEmpty()) {
    try {
        threadCount = Integer.parseInt(threadCountStr)
        if (threadCount &lt;= 0) {
            throw new NumberFormatException()  // Ensure positive number
        }
    } catch (NumberFormatException e) {
        println(&quot;Error: Invalid threadCount value. It should be a positive integer. Exiting.&quot;)
        System.exit(1)  // Exit with error code 1 if the value is invalid
    }
} else {
    println(&quot;Error: threadCount is not set! Exiting.&quot;)
    System.exit(1)  // Exit with error code 1 if threadCount is not provided
}

// Check if environment is still empty, set to an empty string
if (environment.isEmpty()) {
    environment = &quot;&quot;  // Set environment to empty string if not provided
}

// Log the values for debugging (optional)
println(&quot;Thread Count: &quot; + threadCount)
println(&quot;Environment: &quot; + environment)

// Set the threadCount and environment as JMeter properties (accessible globally)
props.put(&quot;threadCount&quot;, threadCount)
props.put(&quot;environment&quot;, environment)

// You can use the environment to set different values based on the environment
String serviceName
switch (environment) {
    case &quot;uat&quot;:
        serviceName = &quot;bff.black-sg.com&quot;
        break
    case &quot;prod&quot;:
        //serviceName = &quot;bff.blusg.com&quot; //Disabled to avoid any Accicdental run in Prod
        break
    default:
        serviceName = &quot;bff.int-lt&quot;; // Default is Load test env
        portNumber = &quot;3000&quot;
        protocol = &quot;http&quot;
}

// Set the serviceName to a JMeter property for HTTP requests
props.put(&quot;serviceName&quot;, serviceName)
props.put(&quot;portNumber&quot;, portNumber);
props.put(&quot;protocol&quot;, protocol);

// Generate executionId as a UUID
String executionId = &quot;lt_&quot; + UUID.randomUUID().toString()  // Format executionId with UUID
props.put(&quot;executionId&quot;, executionId)  // Set executionId as JMeter property

// Generate Start date
// Get the current system time in the required format
def currentDateTime = new SimpleDateFormat(&quot;yyyy-MM-dd&apos;T&apos;HH:mm:ss.SSSZ&quot;).format(Calendar.getInstance().getTime())

// Set the current date and time as a JMeter property
props.put(&quot;startDateTime&quot;, currentDateTime)

// Print the current date and time (for debugging purposes)
println(&quot;Current startDateTime: &quot; + currentDateTime)



// Reports Out dir and files
// Use CircleCI&apos;s current working directory to set aggregate report path
String circleCiWorkingDir = System.getenv(&quot;CIRCLE_WORKING_DIRECTORY&quot;) ?: System.getProperty(&quot;user.dir&quot;)  // Fallback to user.dir if not set
String jmeterResultsDir = vars.get(&quot;jmeterResultsDir&quot;)

// Set the aggregate report path as a JMeter property
String aggregateReportPath = &quot;${circleCiWorkingDir}/${jmeterResultsDir}/aggregate reports&quot;
props.put(&quot;aggregateReportPath&quot;, aggregateReportPath)

// Check if the directory exists, if not, create it
File reportDir = new File(aggregateReportPath)
if (!reportDir.exists()) {
    boolean created = reportDir.mkdirs()  // Create the directory if it doesn&apos;t exist
    if (created) {
        println(&quot;Directory created: &quot; + aggregateReportPath)
    } else {
        println(&quot;Failed to create directory: &quot; + aggregateReportPath)
    }
}

String aggregateReportFilePath = aggregateReportPath + &quot;/${executionId}_${threadCount}.csv&quot;
println(&quot;Aggregate Report File path: &quot; + aggregateReportFilePath)  // Log the generated path for debugging
props.put(&quot;aggregateReportFilePath&quot;, aggregateReportFilePath)

// Set the aggregate report path as a JMeter property
String resultsTreePath = &quot;${circleCiWorkingDir}/${jmeterResultsDir}/results tree&quot;
props.put(&quot;resultsTreePath&quot;, resultsTreePath)

// Check if the directory exists, if not, create it
reportDir = new File(resultsTreePath)
if (!reportDir.exists()) {
    boolean created = reportDir.mkdirs()  // Create the directory if it doesn&apos;t exist
    if (created) {
        println(&quot;Directory created: &quot; + aggregateReportPath)
    } else {
        println(&quot;Failed to create directory: &quot; + aggregateReportPath)
    }
}

String resultsTreeFilePath = resultsTreePath + &quot;/${executionId}_${threadCount}.csv&quot;
println(&quot;results Tree File path: &quot; + resultsTreeFilePath)  // Log the generated path for debugging
props.put(&quot;resultsTreeFilePath&quot;, resultsTreeFilePath)
</stringProp>
