          <JSR223PreProcessor guiclass="TestBeanGUI" testclass="JSR223PreProcessor" testname="User and Env Set Up">
            <stringProp name="cacheKey">true</stringProp>
            <stringProp name="filename"></stringProp>
            <stringProp name="parameters"></stringProp>
            <stringProp name="script">// Fetch the current thread number (1-based index)
int threadNumber = ctx.getThreadNum() + 1 // JMeter thread numbers are zero-based

// Get environment from JMeter properties (set as User Defined Variable or via CLI)
String environment = vars.get(&quot;environment&quot;) ?: &quot;&quot; // default to load test env if not provided
String protocol = &quot;https&quot;
String portNumber = &quot;&quot;

// Define serviceName based on environment
String serviceName;
switch (environment) {
    case &quot;uat&quot;:
        serviceName = &quot;bff.black-sg.com&quot;;
        break;
    case &quot;prod&quot;:
        serviceName = &quot;bff.blusg.com&quot;;
        break;
    default:
        serviceName = &quot;bff.int-lt&quot;; // Default is Load test env
        portNumber = &quot;3000&quot;
        protocol = &quot;http&quot;
}

// Set service and port variables for HTTP request
vars.put(&quot;serviceName&quot;, serviceName);
vars.put(&quot;portNumber&quot;, portNumber);
vars.put(&quot;protocol&quot;, protocol);

// Static domain name
String domain = &quot;bluesg.com.sg&quot;

// Read userName from CSV and create userEmail by appending the domain
String userEmail = vars.get(&quot;userName&quot;) + &quot;@&quot; + domain

// Standard password format (constant for all threads)
String userPassword = &quot;Loadtesting@01&quot;

//Debugging only
//userEmail = &quot;sjbsgprod01@gmail.com&quot;
//userPassword = &quot;Tester@1&quot;

// Store variables for use in HTTP requests
vars.put(&quot;userEmail&quot;, userEmail)
vars.put(&quot;userPassword&quot;, userPassword)
vars.put(&quot;stationId&quot;, vars.get(&quot;sourceId&quot;))  // From CSV
vars.put(&quot;carCategory&quot;, vars.get(&quot;carCategory&quot;))  // From CSV

// Print carId to the JMeter log
println(&quot;Thread csv test data &quot; + threadNumber + &quot; is: &quot; + userEmail + &quot; &quot; + vars.get(&quot;stationId&quot;) + &quot; &quot; + vars.get(&quot;carCategory&quot;));
</stringProp>
            <stringProp name="scriptLanguage">groovy</stringProp>
          </JSR223PreProcessor>
